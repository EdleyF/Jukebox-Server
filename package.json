{
  "name": "spotify_app",
  "version": "1.0.0",
  "description": "App that connects to spotify API to do enable OSC members access to music",
  "main": "server.js",
  "scripts": {
    "test": "npm run build && mocha --reporter spec --recursive",
    "start": "node ./dist/src/server.js",
    "dev": "nodemon -L -e ts --exec \"npm run build && node ./dist/src/swagger.js && npm start\"",
    "build": "tsc",
    "swagger-autogen": "npm run build && node ./dist/src/swagger.js",
    "config": "node utils/config.js",
    "playground": "nodemon playground/server.js",
    "docker-cli": "docker exec -it api sh"
  },
  "author": "",
  "license": "ISC",
  "mocha": {
    "spec": "./dist/test"
  },
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "pg": "^8.10.0",
    "querystring": "^0.2.1",
    "request": "^2.88.2",
    "swagger-autogen": "^2.23.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/mocha": "^10.0.1",
    "@types/pg": "^8.6.6",
    "chai": "^4.3.7",
    "chai-http": "^4.3.0",
    "child_process": "^1.0.2",
    "dedent-js": "^1.0.1",
    "fs": "^0.0.1-security",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.21",
    "prompt-sync": "^4.2.0",
    "typescript": "^4.9.5",
    "util": "^0.12.5"
  }
}
